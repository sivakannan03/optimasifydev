@{
    ViewBag.Title = "PCLtoMCLRequest";
    Layout = "~/Views/Shared/MasterLayout.cshtml";
}

<style>
   

   select {
        width: 250px;
    }
    #PCLtoMCLGrid_chkbox .s-ico span {
        display: none;
    }
     
</style>



<body>

    @using (Html.BeginForm(FormMethod.Post))
    {
        <div>
            
       <div style="text-align:center;"><label id="lblError" style=" color:red"></label></div>
              
            <table width="100%" class="inner_tb">
                <tr>
                    <th colspan="8">PCL to MCL Request </th>
                </tr>
                <tr>
                    <td class="tb-r1" style="width:5%;">
                        Search
                    </td>
                    <td class="tb-r1" style="width:5%;">Brick</td>
                    <td class="tb-r1" style="width:20%;">
                        @Html.ListBox("lstBrick", PfizerEntity.UsefulLink.GetBricks(Convert.ToInt32(@Session["Territory_FKID"].ToString())), "All")

                    </td>
                    <td class="tb-r1" style="width:10%;">Customer Type</td>

                    <td class="tb-r1" style="width:20%;">
                        <select name="lstCustomer" id="lstCustomer" multiple style=" width:200px !important;">
                            <option value="All">All</option>
                            <option value="Doctor">Doctor</option>
                            <option value="Chemist">Chemist</option>
                            <option value="Nurse">Nurse</option>
                            <option value="Patient">Patient</option>
                            <option value="Hospital">Hospital</option>
                            <option value="Stockist">Stockist</option>
                        </select>

                    </td>
                    <td class="tb-r1" style="width:5%;">Status</td>
