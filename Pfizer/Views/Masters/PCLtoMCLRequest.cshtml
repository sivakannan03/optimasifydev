                        <select name="lstStatus" id="lstStatus" style=" width:140px !important;">
                            <option value="">Select</option>
                              <option value="New Request" selected>New Request</option>
                            <option value="Pending Request">Pending Request</option>
                            <option value="Approved Request">Approved Request</option>
                            <option value="Rejected Request">Rejected Request</option>
                        </select>

                    </td>
                    <td class="tb-r1" style="width:5%;"><input type='button' class=" blue_button" value="Fetch" name="btnFetch" id="btnFetch" /></td>
                </tr>

            </table>

            <table border="0" style="width: 100%;"> 

                <tr>
                    <td>
                        <div id="divNew" style=" display:none">
                            <!--JqGrid Starts-->
                            <table style="margin-top: 7px; width: 100%; " border="0">
                                <tr>
                                    <td>
                                        <table id="PCLtoMCLGrid" style="width: 100% ;" border="0"></table>
                                        
                                         
                                    </td>
                                </tr>
                            </table>
                        </div>
                        <!--Ends-->
                    </td>
                </tr>

                <tr>
                    <td>
                        <!--JqGrid Starts-->
                        <div id="divPending" style=" display:none">
                            <table style="margin-top: 7px; width: 100%; " border="0">
                                <tr>
                                    <td>
                                        <table id="PCLtoMCLPendingGrid" style="width: 100%; " border="0"></table>
                                        <div id="PendingGridPager">
                                        </div>
                                    </td>
                                </tr>
                            </table>
                        </div>
                            <!--Ends-->
</td>
                </tr>

                <tr>
                    <td>
                        <!--JqGrid Starts-->
                        <div id="divApproved" style=" display:none">
                            <table style="margin-top: 7px; width: 100%; " border="0">
                                <tr>
                                    <td>
                                        <table id="PCLtoMCLApprovedGrid" style="width: 100%; " border="0"></table>
                                        <div id="ApprovedGridPager">
                                        </div>
                                    </td>
                                </tr>
                            </table>
                        </div>
                            <!--Ends-->
</td>
                </tr> 

                <tr>
                    <td>
                        <!--JqGrid Starts-->
                        <div id="divRejected" style=" display:none">
                            <table style="margin-top: 7px; width: 100%; " border="0">
                                <tr>
                                    <td>
                                        <table id="PCLtoMCLRejectedGrid" style="width: 100%; " border="0"></table>
                                        <div id="RejectedGridPager">
                                        </div>
                                    </td>
                                </tr>
                            </table>
                        </div>
                            <!--Ends-->
</td>
                </tr>

                <tr>
                    <td align="center"><br />
                        <input type="button" class="orange_button" value="Print" id="btnPrint" />
                        &nbsp;&nbsp;<input type="button" class="orange_button" value="Submit" id="btnSubmit" />
                        &nbsp;&nbsp;<input type="button" class="orange_button" value="Cancel" id="btnCancel" />
                
                    </td>
                </tr>
            </table>

        </div>
        <div id="openDiv" style="display:none">
            
             
        </div>
        
           <!-- Doctor Profile Start-->
        
        <div class="inner_tbldiv" id="DoctorDiv" style="display: none; margin-top:10px !important;" >
        <table border="0" style="width: 600px"> 


            <tr style=" vertical-align:top" id="trDoctor">
                <td width="2%"></td>
                <td> 
                    <table class="inner_tb" id="tblHeader" width="100%" cellpadding="0" cellspacing="0">
                        <tr id="tr_Name">
                            <td class="tb-r1">FirstName :</td>
                            <td class="tb-r1"> </td>
                            <td class="tb-r1">MiddleName :</td>
                            <td class="tb-r1"></td>
                            <td class="tb-r1">LastName :</td>
                            <td class="tb-r1"></td>
                        </tr>
                        <tr id="tr_Address">
                            <td class="tb-r2">Address :</td>
                            <td class="tb-r2"> </td>
                            <td class="tb-r2">Specialization :</td>
                            <td class="tb-r2" colspan="3"></td>
                        </tr>

                        <tr id="tr_Street">
                            <td class="tb-r1">Street :</td>
                            <td class="tb-r1"> </td>
                            <td class="tb-r1">Class :</td>
                            <td colspan="3" class="tb-r1"></td>
                        </tr>
                        <tr id="tr_Area">
                            <td class="tb-r2">Area :</td>
                            <td colspan="5" class="tb-r2"> </td>

                        </tr>

                        <tr id="tr_Zip">
                            <td class="tb-r1">Zip :</td>
                            <td colspan="5" class="tb-r1"> </td>
                        </tr>
                        <tr id="tr_Phone">
                            <td class="tb-r2">Phone(R) :</td>
                            <td class="tb-r2"> </td>
                            <td class="tb-r2">Phone(Clinic) :</td>
                            <td class="tb-r2" colspan="3"></td>
                        </tr>
                        <tr id="tr_Mobile">
                            <td class="tb-r1">Mobile :</td>
                            <td class="tb-r1"> </td>
                            <td class="tb-r1">e-Mail :</td>
                            <td colspan="3" class="tb-r1"></td>
                        </tr>
                    </table>

                </td>
                <td></td>
                <td width="2%"></td>

            </tr>
            <!-- Chemist Profile Starts-->
            <tr id="trChemist">
                <td width="2%"></td>
                <td>
                    <table class="inner_tb" id="tblChemist" width="100%" cellpadding="0" cellspacing="0">
                        <tr id="tr_CheName">
                            <td class="tb-r1">FirstName :</td>
                            <td class="tb-r1"> </td>
                            <td class="tb-r1">MiddleName :</td>
                            <td class="tb-r1"></td>
                            <td class="tb-r1">LastName :</td>
                            <td class="tb-r1"></td>
                        </tr>
                        <tr id="tr_CheAddress">
                            <td class="tb-r2">Address :</td>
                            <td class="tb-r2"> </td>
                            <td class="tb-r2">Street :</td>
                            <td class="tb-r2" colspan="3"></td>
                        </tr>


                        <tr id="tr_CheArea">
                            <td class="tb-r2">Area :</td>
                            <td colspan="5" class="tb-r2"> </td>

                        </tr>

                        <tr id="tr_CheZip">
                            <td class="tb-r1">Zip :</td>
                            <td colspan="5" class="tb-r1"> </td>
                        </tr>
                        <tr id="tr_ChePhone">
                            <td class="tb-r2">Phone(R) :</td>
                            <td class="tb-r2"> </td>
                            <td class="tb-r2">Mobile :</td>
                            <td class="tb-r2" colspan="3"></td>
                        </tr>
                        <tr id="tr_CheMail">
                            <td class="tb-r2">e-Mail :</td>
                            <td colspan="5" class="tb-r1"> </td>

                        </tr>
                    </table>

                </td>
                <td width="2%"></td>

            </tr>

            <tr>
                <td width="2%"></td>
                <td>

                   
                    <table style="margin-top: 7px; width :600px" border="0">
                        <tr>
                            <td>
                                <table id="DoctorInfoGrid" style="width: 600px;" border="0"></table>
                                <div id="DoctorPager">
                                </div>
                            </td>
                        </tr>
                    </table>
                   
                </td>
                <td width="2%"></td>
            </tr>



        </table>
    </div>
        
        <!-- Doctor Profile end-->

        <script type="text/javascript"> 
          
            $('#btnCancel').click(function () {
                
                $("#lstBrick option").prop("selected", false);
                $("#lstCustomer option").prop("selected", false);
                $('#lstStatus').val('');
                $('#divNew').css('display', 'none');
                $('#divPending').css('display', 'none');
                $('#divApproved').css('display', 'none');
                $('#divRejected').css('display', 'none');
                
            });

            function DisplayError(type) {
                 
                $('#openDiv').css('display', 'block');
                if (type == 'notselect')
                    $('#openDiv').html('Please Select atleast one row');
                else
                    $('#openDiv').html('Under Construction');
                
                
                $('#openDiv').dialog({
                    title: 'Alert Message',
                    height: 78,
                    width: 308,
                    show: { effect: 'drop', direction: "up" },
                    modal: true,
                    open: function (event, ui) {

                    },

                });


            } 

            $("#lstBrick").on('change', function () {
                if ($(this).val() == "All") {
                    $("#lstBrick option").prop("selected", true); 
                    $("#lstBrick option:first").removeAttr('selected');
                }
            });

            $("#lstCustomer").on('change', function () {
               
                if ($(this).val() == "All") {
                    $("#lstCustomer option").prop("selected", true);
                    $("#lstCustomer option:first").removeAttr('selected');
                }
            });

            $("#lstStatus").on('change', function () {
                $("#txtStatus").val($(this).val()); 
            });

            
         

            jQuery(document).ready(function () {
                var CusTypeXML = "";
                var AreaFKID = "";
                $("#lstBrick option:first").removeAttr('selected');
                $("#lstCustomer option").prop("selected", true);
                $("#lstCustomer option:first").removeAttr('selected');
                AreaFKID = $('#lstBrick').val();
                $('#divNew').css('display', 'block');
                initGrid(CusTypeXML, AreaFKID, 0);
                
                $('#lblError').text('');
                

            });

            function checkBox(e) { 

                e = e || event;/* get IE event ( not passed ) */
                e.stopPropagation ? e.stopPropagation() : e.cancelBubble = true;
                var $checkBoxes = $('input[type="checkbox"]');
                var check = !$(this).data('checked');
                $checkBoxes.prop('checked', check);
                $(this).data('checked', check); 
              
            }


            function DoctorProfile(MASPKID, Type) {
                
                if ((Type == "Nurse") || (Type == "Patient"))
                {
                   
                    DisplayError('Nurse');
                  
                }
                else {

                    //Get Doctor Information :
 
                    if (Type == 'Doctor') {
                        $.getJSON('@Url.Action("JsonGetDoctorDetails")?MASPKID=' + MASPKID + '&Type=' + Type + '&flag=' + 'header', function (data) {
                            $("#trDoctor").show();
                            $("#trChemist").hide();
                            jQuery('#tr_Name  td:nth-child(2)').html(data.FName);
                            jQuery('#tr_Name  td:nth-child(4)').html(data.MName);
                            jQuery('#tr_Name  td:nth-child(6)').html(data.LName);

                            jQuery('#tr_Address  td:nth-child(2)').html(data.Address);
                            jQuery('#tr_Address  td:nth-child(4)').html(data.Specialization);


                            jQuery('#tr_Street  td:nth-child(2)').html(data.Street);
                            jQuery('#tr_Street  td:nth-child(4)').html(data.AreaFKID);

                            jQuery('#tr_Area  td:nth-child(2)').html(data.AreaName);

                            jQuery('#tr_Zip  td:nth-child(2)').html(data.Zipcode);
                            jQuery('#tr_Phone  td:nth-child(2)').html(data.TelRoles);
                            jQuery('#tr_Phone  td:nth-child(4)').html(data.TelClinic);

                            jQuery('#tr_Mobile  td:nth-child(2)').html(data.Mobile);
                            jQuery('#tr_Mobile  td:nth-child(4)').html(data.Email);

                        });


                    }
                    else if (Type == 'Chemist')
                    {
                       $.getJSON('@Url.Action("JsonGetDoctorDetails")?MASPKID=' + MASPKID + '&Type=' + Type + '&flag=' + 'header', function (data) {
                       $("#trChemist").show();
                        $("#trDoctor").hide();
                        jQuery('#tr_CheName  td:nth-child(2)').html(data.FName);
                        jQuery('#tr_CheName  td:nth-child(4)').html(data.MName);
                        jQuery('#tr_CheName  td:nth-child(6)').html(data.LName);

                        jQuery('#tr_CheAddress  td:nth-child(2)').html(data.Address);
                        jQuery('#tr_CheAddress  td:nth-child(4)').html(data.Street);
                        jQuery('#tr_CheArea  td:nth-child(2)').html(data.AreaName);

                        jQuery('#tr_CheZip  td:nth-child(2)').html(data.Zipcode);
                        jQuery('#tr_ChePhone  td:nth-child(2)').html(data.PhoneNo);
                        jQuery('#tr_ChePhone  td:nth-child(4)').html(data.Mobile);
                        jQuery('#tr_CheMail  td:nth-child(2)').html(data.Email);
                        });
                    }
                    $('#DoctorDiv').css('display', 'block');

                    $('#DoctorInfoGrid').jqGrid('GridUnload');
                    jQuery("#DoctorInfoGrid").jqGrid({
                        url: '@Url.Action("JsonGetDoctorDetailsforGrid")?MASPKID=' + MASPKID + '&Type=' + Type ,
                        datatype: "json",
                        jsonReader: { repeatitems: false, id: "Name" },
                        colNames: ['Name', 'Date'],
                        colModel: [

                            { name: 'LoginName', index: 'LoginName', align: 'left' },
                            { name: 'Date', index: 'Date', align: 'left' }
                        ],

                        caption: 'Call History',
                        gridview: true,
                        jqModal: true,
                        rowTotal: 10,
                        rowNum: 5,
                        width:600,
                        align: 'center',
                        sortable: true,
                        hidegrid: true,
                        altRows: true,
                        viewrecords: true,
                        pager: $('#DoctorPager'),
                        height: 'auto' 

                    });


                    $('#DoctorDiv').dialog({
                        title:'Daily Plan',
                        width: 630,
                        height:500,
                        show: { effect: 'drop', direction: "up" },
                        modal: true,
                        open: function (event, ui) {

                        },

                    });

                   
                }
            }
             

            var lastsel;
            function initGrid(CusTypeXML, AreaFKID, status) { 
                if (status == '0')
                    status = 'New Request'
                else
                    status = $('#lstStatus').val();

                jQuery('#PCLtoMCLGrid').jqGrid('GridUnload');
                jQuery("#PCLtoMCLGrid").jqGrid({

                    url: '@Url.Action("PCLtoMCLGridData")?CusTypeXML=' + $('#lstCustomer').val() + '&AreaFKID=' + AreaFKID,
                    jsonReader: { repeatitems: false, id: "ID" },
                    datatype: "json",
                     colNames: ['<input type="checkbox"  onclick="checkBox(event)"/>', 'Customer Name', 'Cus. Type', 'Brick', 'Speciality', 'Reason for Request', '',''],
                    
                    colModel: [                   
                     {
                         name: 'chks', index: 'chks',width:30,  align: "center", editable: true,
                         edittype: "checkbox", editoptions: { value: "Yes:No" }, formatter: "checkbox", formatoptions: { disabled: false }
                     },
                        { name: 'CustomerName', index: 'CustomerName', align: 'left', hidden: false, editable: false, formatter: 'showlink'},
                        { name: 'Type', index: 'Type', align: 'left', editable: false},
                        { name: 'AreaName', index: 'AreaName', align: 'left', hidden: false, editable: false },
                        { name: 'Specialization', index: 'Specialization', align: 'left', hidden: false, edit: false },
                        {
                            name: 'Reason', index: 'Reason', align: 'left', hidden: false, editable: true, edittype: 'text',  sortable: true, editoptions: {
                                
                                dataEvents: [{
                                    type:'keyup', // blur
                                    fn: function (e) {
                                        SingleCharacter(this); 
                                     
                                    },

                                }],
                            }

                           
                        },
                    { name: 'MASPKID', index: 'MASPKID', align: 'left', hidden: true, editable: false },
                      { name: 'ID', index: 'ID', align: 'left', hidden: true, editable: false },
                    ], 
                    
                    autowidth: true,
                   
                    hidegrid: false,
                    altRows: true,
                    rowTotal: 10,
                    height: 250,
                    loadonce: true,
                    sortname: 'ID',
                    sortorder: 'asc',
                    rowNum: 100000,
                    viewrecords: true,
                    onSelectRow: function (id) {
                      
                        var ch = jQuery(this).find('#' + id + ' input[type=checkbox]').attr('checked');
                        if (ch) {
                            jQuery(this).find('#' + id + ' input[type=checkbox]').attr('checked', false);
                        } else {
                            jQuery(this).find('#' + id + ' input[type=checkbox]').attr('checked', true);
                        }
                        rowChecked = 1;
                        currentrow = id;
                        if (!$("input[type=checkbox]").is(':checked') && !$("#chks").is(':checked')) {
                            jQuery('#PCLtoMCLGrid').jqGrid('saveRow', id);

                        }
                        else {
                            jQuery("#PCLtoMCLGrid").jqGrid('editRow', id);

                        }

                        if (id != lastsel) {
                            jQuery('#PCLtoMCLGrid').saveRow(lastsel);
                            lastsel = id;
                        }
                        jQuery('#PCLtoMCLGrid').editRow(id, true);

                      
                   
                    },
                    afterInsertRow: function (rowid, rowData, rowelem) {
                     
                        var detail = rowData['Type'];
                        if (detail == "Doctor" || detail == "Chemist") {
                            $("#PCLtoMCLGrid").jqGrid('setRowData', rowid, false, { color: '#2f72c3' });
                        } else if (detail == "Nurse") { 
                            $("#PCLtoMCLGrid").jqGrid('setRowData', rowid, false, { color: '#E77471' });
                        }
                    },
                    loadComplete: function () {
                       
                        var ids = jQuery("#PCLtoMCLGrid").jqGrid('getDataIDs');
                        var c1 = "", c2 = "", c5 = "";
                        
                            if (jQuery("#PCLtoMCLGrid").getGridParam("records") == 0) {
                                jQuery("#PCLtoMCLGrid").addRowData(
                                "blankRow", { "Title": "No records to view" });
                            }
                           
                            for (var i = 0; i < ids.length; i++) {

                                 c1 = jQuery('#PCLtoMCLGrid').jqGrid('getCell', ids[i], 1);
                                 c2 = jQuery('#PCLtoMCLGrid').jqGrid('getCell', ids[i], 2);
                                 c5 = jQuery('#PCLtoMCLGrid').jqGrid('getCell', ids[i], 6);
                                
                                 if (c2 != 'Hospital' && c2 != 'Stockist' && c2 != 'Nurse') {
                                     popUp1 = "<a onclick=\"javascript:DoctorProfile('" + c5 + "','" + c2 + "');\"  style=\"color:#2f72c3;font-weight:bold;cursor:pointer\">" + c1 + "</a>";
                                     jQuery("#PCLtoMCLGrid").jqGrid('setRowData', ids[i], { CustomerName: popUp1 });
                                 }
                                 else if (c2 == 'Nurse') { 
                                     
                                     popUp2 = "<span  onclick=\"javascript:DoctorProfile('" + c5 + "','" + c2 + "');\" style=\"color:#E77471 !important;font-weight:bold;\">" + c1 + "</span>";
                                     jQuery("#PCLtoMCLGrid").jqGrid('setRowData', ids[i], { CustomerName: popUp1 });
                                 }

                                 else if (c2 == 'Hospital' || c2 == 'Stockist') {
                                     popUp3 = "<a style=\"font-weight:bold;\">" + c1 + "</a>";
                                     jQuery("#PCLtoMCLApprovedGrid").jqGrid('setRowData', ids[i], { CustomerName: popUp3 });
                                 }


                            }
                    }, 
                    editable: true 
                }); 
            }  
 
            $('#btnSubmit').click(function () {
                var selr = jQuery('#PCLtoMCLGrid').jqGrid('getGridParam', 'selrow');
                
                jQuery('#PCLtoMCLGrid').jqGrid('saveRow', selr);
                var gridData = jQuery("#PCLtoMCLGrid").getRowData();
                var postData = JSON.stringify(gridData); 
          
                    $.ajax({
                        url: '@Url.Action("SubmitPCLRequest")',
                        type: 'POST',
                        dataType: 'json',
                        data: { gridData: postData },
                        success: function (result) {
                            if (result == 'notselect') {
                                DisplayError(result);
                            } else {
                                $('#lblError').text(result);
                                $("#PCLtoMCLGrid").setGridParam({ datatype: 'json', page: 1 }).trigger('reloadGrid');
                                return true;
                            }
                           
                        }
                    });
                  
            });

            $('#btnFetch').click(function () {
                $('select').css("border", "1px solid #C0C0C0"); 
                if ($("#lstBrick").val() == '' || $("#lstBrick").val() == null) {
                    $('#lblError').text('Please select Brick');
                    $("#lstBrick").css("background-color", "#FFE8E8").css("border", "1px solid #FF6666");
                    $('#lstBrick').val('Please select Brick');
                    $('#lstBrick').focus();
                    
                    return false;
                }
                else if ($("#lstCustomer").val() == '' || $("#lstCustomer").val() == null) {
                    $('#lblError').text('Please select Customer Type');
                    $("#lstCustomer").css("background-color", "#FFE8E8").css("border", "1px solid #FF6666");
                    $('#lstCustomer').val('');
                    $('#lstCustomer').focus();
                    return false;

                }
                else if ($("#lstStatus").val() == '') {
                    $('#lblError').text('Please select Status');
                    $("#lstStatus").css("background-color", "#FFE8E8").css("border", "1px solid #FF6666");
                    $('#lstStatus').val('');
                    $('#lstStatus').focus();
                    return false;
                }



                $('#divNew').css('display', 'none');
                $('#divPending').css('display', 'none');
                $('#divApproved').css('display', 'none');
                $('#divRejected').css('display', 'none');

                if ($('#lstStatus').val() == 'Pending Request') {
                    $('#divPending').css('display', 'block');
                    PendingGrid($('#lstCustomer').val(), $('#lstBrick').val(), $('#lstStatus').val());
                    $('#btnPrint').hide();
                    $('#btnSubmit').hide();
                    $('#btnCancel').hide();
                }
                else if ($('#lstStatus').val() == 'Approved Request') {
                    
                    $('#divApproved').css('display', 'block');
                    ApprovedGrid($('#lstCustomer').val(), $('#lstBrick').val(), $('#lstStatus').val());
                    $('#btnPrint').hide();
                    $('#btnSubmit').hide();
                    $('#btnCancel').hide();
                }
                else if ($('#lstStatus').val() == 'Rejected Request') {
                    $('#divRejected').css('display', 'block');
                    RejectedGrid($('#lstCustomer').val(), $('#lstBrick').val(), $('#lstStatus').val());
                    $('#btnPrint').hide();
                    $('#btnSubmit').hide();
                    $('#btnCancel').hide();
                }
                else if ($('#lstStatus').val() == 'New Request') {
                    $('#divNew').css('display', 'block');
                    initGrid($('#lstCustomer').val(), $('#lstBrick').val(), $('#lstStatus').val());
                    $('#btnPrint').show();
                    $('#btnSubmit').show();
                    $('#btnCancel').show();
                
                }


            });


            function PendingGrid(CusTypeXML, AreaFKID, status) {

                 
                $('#PCLtoMCLPendingGrid').jqGrid('GridUnload');
                jQuery("#PCLtoMCLPendingGrid").jqGrid({

                    url: '@Url.Action("PCLPendingGridData")?CusTypeXML=' + $('#lstCustomer').val() + '&AreaFKID=' + AreaFKID,
                   // jsonReader: { repeatitems: false, id: "MASPKID" },
                    datatype: "json",
                    colNames: ['Request Date', 'Customer Name', 'Customer Type', 'Brick', 'Speciality', 'Reason for Request', ''],

                    colModel: [
                        { name: 'CreatedDate', index: 'CreatedDate', align: 'center'},
                        { name: 'CustomerName', index: 'CustomerName', align: 'left', formatter: 'showlink' },
                        { name: 'Type', index: 'Type', align: 'left', editable: false },
                        { name: 'AreaName', index: 'AreaName', align: 'left', hidden: false, editable: false },
                        { name: 'Specialization', index: 'Specialization', align: 'left', hidden: false, edit: false },
                        {
